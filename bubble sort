
#include <stdio.h>

int main()
{
    int item,i,j,temp;
    int a[]={20,50,100,10,90,40,60,5,30,35};


    for(int i=0;i<10;i++)
    {
        for(int j=0;j<10-i;j++)
        {
            if(a[j]>a[j+1])
            {
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;

            }

        }
    }

    for(int k=0;i<10;k++)
    {
        printf("%d",a[k]);
    }
return 0;
}
